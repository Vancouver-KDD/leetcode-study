import re

class Solution:
    def isPalindrome(self, s: str) -> bool:
        clean = re.sub(r'[^a-z0-9]', '', s.lower())

        left, right = 0, len(clean) - 1
        while left < right:
            if clean[left] != clean[right]:
                return False
            left += 1
            right -= 1

        return True
