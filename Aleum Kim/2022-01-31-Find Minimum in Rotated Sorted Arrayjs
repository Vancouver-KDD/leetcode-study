var findMin = function(nums) {

    let left = 0;
    let right = nums.length-1;

    while(left<right) {

        let mid = Math.floor((right+left)/2)

        if(nums[right] < nums[mid]) {
            left = mid+1;
        }else {
            right = mid;
        }
        
    }
    return nums[left];
} 
var findMin = function(nums) {

   return Math.min(...nums)

} 

// Time O(log(N)) | Space O(1)


//find the middle point
// decide go left or right
// check the either
